<!DOCTYPE html>
<html>
<head>
  <title>Fresk</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= stylesheet_link_tag 'http://fonts.googleapis.com/css?family=The+Girl+Next+Door' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>

<body>

  <div class="container">
    <%= render 'top_header' %>

    <%= render 'navbar' %>

    <% [:notice, :alert].each do |level| %>
      <%= stylize_flash_message(level, flash[level]) %>
    <% end %>

    <%= yield %>

    <%= render 'footer' %>

  </div>

</body>
</html>
